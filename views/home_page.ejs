			src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js"
			integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb"
			crossorigin="anonymous"
		></script>
		<script src="/blog_home_assests/js/bootstrap.min.js"></script>
		<script src="/blog_home_assests/js/ie10-viewport-bug-workaround.js"></script>

		<%- include('./partials/footer'); -%>

		<script>
			const tabcontainer = document.getElementById("tab-container");
			const fetaured_arr =<%- JSON.stringify(featured) %>;
			const news_arr =<%- JSON.stringify(news) %>;

			const data = {
				featured: fetaured_arr,
				news: news_arr,
			}

			tabcontainer.addEventListener("click", function (e) {
				const tabid = e.target.id;
				const tab = document.getElementById(tabid);
				const alltabs = document.querySelectorAll("#tab-container span");
				const tabcontent = document.getElementById("tab-content")

				const dataset_key = tab.dataset.name;

				if(!dataset_key){
					return;
				}

				const dataset = data[dataset_key];

				let html_data = ``;

				dataset.forEach(b =>{
					html_data += `
						<div class="card">
						<div class="row">
							<div class="col-md-5 wrapthumbnail">
								<a href="${b.link}" target="_blank">
									<div
										class="thumbnail"
										style="background-image: url(${b.header_image})"
									></div>
								</a>
							</div>
							<div class="col-md-7">
								<div class="card-block">
									<h2 class="card-title">
										<a href="${b.link}" target="_blank"> ${b.title} </a>
									</h2>
									<h4 class="card-text">${b.desc}</h4>
									<div class="metafooter">
										<div class="wrapfooter">
											<span class="meta-footer-thumb">
												<a href="#"
													><img class="author-thumb" src="${b.creator_profile_url}" alt="${b.creator_title}"
												/></a>
											</span>
											<span class="author-meta">
												<span class="post-name"
													><a href="#">${b.creator_title}</a></span
												><br />
												<span class="post-date">
													${b.blog_update_date ? `<time datetime="${b.blog_update_date_iso}">
														last updated at ${b.blog_update_date}
													</time>` : ``}
												</span
												><span class="dot"></span>
												<!-- <span class="post-read">6 min read</span> -->
											</span>
											<!-- <span class="post-read-more"
												><a href="post.html" title="Read Story"
													><svg
														class="svgIcon-use"
														width="25"
														height="25"
														viewbox="0 0 25 25"
													>
														<path
															d="M19 6c0-1.1-.9-2-2-2H8c-1.1 0-2 .9-2 2v14.66h.012c.01.103.045.204.12.285a.5.5 0 0 0 .706.03L12.5 16.85l5.662 4.126a.508.508 0 0 0 .708-.03.5.5 0 0 0 .118-.285H19V6zm-6.838 9.97L7 19.636V6c0-.55.45-1 1-1h9c.55 0 1 .45 1 1v13.637l-5.162-3.668a.49.49 0 0 0-.676 0z"
															fill-rule="evenodd"
														></path></svg></a
											></span> -->
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
					`
				})

				//remove active
				alltabs.forEach((t) => (t.style.borderBottom = "none"));

				//add active
				tab.style.borderBottom = "2px solid rgba(0, 0, 0, .44)";

				tabcontent.innerHTML = html_data;
			});
		</script>
	</body>
